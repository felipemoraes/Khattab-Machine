DIV_FUN:	PUSH	R3	
PUSH	R1	
XOR	R2	R2
LOAD	R3	ONE
COPY 	R5	R1
COPY	R6	R1
AND 	R5	R6
JZ	END
COPY 	R5	R2
COPY	R6	R0
SUB 	R5	R6	
JZ	L0	
STORE	R3	SN
LOAD	R7	ZERO
SUB	R7	R0
COPY	R0	R7	
L0:	COMP	R2	R1
BLZ	L1	
STORE	SD	R3
LOAD	R7	ZERO
SUB	R7	R1
COPY	R1	R7	
L1:	COMP	R0	R1
BLZ	L3	
L2:	SUB	R0	R1
ADD	R2	R3
COMP	R0	R1
BLZ	L3	
JMP	L2	
L3:	LOAD	R3	SN
TEST	R3	R3
BEZ	L5	
TEST	R0	R0
BEZ	L4	
NEG	R0	
ADD	R0	R1
ADD	R2	R3
L4:	NEG	R2	
L5:	LOAD	R3	SD
TEST	R3	R3
BEZ	L6	
NEG	R2	
L6:	XOR	R0	R2
XOR	R2	R0
XOR	R0	R2
END:	POP	R1	
POP	R3	
RET		
SN:	WORD	0	
SD:	WORD	0
ZERO:	WORD	0		
ONE:	WORD	1	
MACROAUX:	WORD	1	
END
