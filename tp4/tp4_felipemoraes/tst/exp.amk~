EXP_FUN:	PUSH	R1	
PUSH	R2	
PUSH	R3	
LOAD	R3	ONE
LOAD	R2	ONE
COPY 	R5	R1
AND	R5	R5
JZ	pX
COPY 	R5	R1
COPY 	R6	R2
AND	R5	R6	
JNZ	p1	
p0:	CALL	SQUARE	
ADD	R2	R2
COPY 	R5	R1
COPY 	R6	R2
AND	R5	R6
JZ	p0	
p1:	PUSH	R1	
COPY	R1	R0
COPY	R0	R3
CALL	MUL_FUN	
COPY	R3	R0
COPY	R0	R1
POP	R1	
SUB	R1	R2
COPY 	R5	R1
COPY 	R6	R1
AND	R5	R6
JNZ	p0	
pX:	COPY	R0	R3
POP	R3	
POP	R2	
POP	R1	
RET		
ONE:	WORD	1	
END
