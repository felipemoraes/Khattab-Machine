CALC:	READ	R0
READ	R1
READ	R2
ADDF:	LOAD	R3	INT1
COPY 	R5	R2
COPY 	R6	R3
SUB	R5	R6
JNZ	SUBF	
CALL	ADD_FUN	
WRITE	R0
JMP	DONE	
SUBF:	LOAD	R3	INT2
COPY 	R5	R2
COPY 	R6	R3
SUB	R5	R6
JNZ	MULF	
CALL	SUB_FUN	
WRITE	R0	
JMP	DONE	
MULF:	LOAD	R3	INT3
COPY 	R5	R2
COPY 	R6	R3
SUB	R5	R6
JNZ	DIVF	
CALL	MUL_FUN	
WRITE	R0	
JMP	DONE	
DIVF:	LOAD	R3	INT4
COPY 	R5	R2
COPY 	R6	R3
SUB	R5	R6
JNZ	EXPF	
COPY 	R5	R1
AND	R5	R5
JZ	EXPF	
CALL	DIV_FUN	
WRITE	R0	
WRITE	R2	
JMP	DONE	
DONE:	HALT		
INT1:	WORD	1	
INT2:	WORD	2	
INT3:	WORD	3	
INT4:	WORD	4	
INT5:	WORD	5	
ONE:	WORD	1	
END
